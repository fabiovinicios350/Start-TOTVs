Function Main()

    //Variaveis
    LOCAL nValor1
    LOCAL nValor2


    //Obrtendo os valores das variaveis
    ACCEPT "Digite o Valor 01: " TO nValor1
    ACCEPT "Digite o Valor 02: " TO nValor2

    //Apresentando na tela os resultados
    QOUT("Valor 01: " + nValor1)
    QOUT("Valor 02: " + nValor2)


Return nil
